{
  "name": "zendesk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/zendesk",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/zendesk",
        "outputPath": "dist/apps/zendesk"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/zendesk"
        },
        "production": {}
      }
    },
    "postbuild": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "next-sitemap --config apps/zendesk/next-sitemap.js"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "zendesk:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "zendesk:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "zendesk:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "zendesk:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/zendesk/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/zendesk/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
